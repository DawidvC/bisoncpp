#include "generator.ih"

void Generator::debugMapDecl(ostream &out) const
{
    if (!d_debug)
        return;

    key(out);

    out <<  
        "    typedef std::map<int, char const *> SMap;\n"
        "    typedef SMap::value_type SMapVal;\n"
        "    static SMapVal s_symArr[];\n"
        "    static SMap s_symbol;\n";
}
