optComma:
    ','
|
    // empty
;

optNumber:
    NUMBER
    {
        $$ = true;
    }
|
    {
        $$ = false;
    }
;

optSemiCol:
    ';'
|
    // empty
;

_token:
    {
        d_expect = "token name";
    }
;

optTypename:
    typename_a
    _token
|
    _token
    {
        d_field.clear();
    }
;

optTwo_percents:
    TWO_PERCENTS
    {
        lineMsg(wmsg) << 
            "Ignoring all input beyond the second %% token" << endl;
        ACCEPT();
    }
|
    // empty
;
