void \@Base::error__()
{
$insert 4 debug "\nERROR with d_recovery " << d_recovery

    if (not d_recovery)
        d_recovery = true;
    else
    {
$insert 8 debug "cannot process " << symbol__(d_token) << ", requesting the next token"
        d_token = _UNDETERMINED_;
    }

    d_reducedToken = _error_;
}
