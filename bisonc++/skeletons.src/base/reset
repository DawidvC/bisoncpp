void \@Base::reset__()
{
    d_nErrors__ = 0;

    d_stackIdx = -1;
    d_stateStack.clear();
    d_state = 0;

    d_acceptedTokens__ = d_requiredTokens__;
    d_token = _UNDETERMINED_;
    d_val__ = STYPE__{};

    d_next.first = _UNDETERMINED_;
    d_skipToken = false;
    d_recovery = false;

    push(0);
}
