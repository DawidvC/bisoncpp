    /*
        pxstring selects either pstring, xstring or string
    */
<pxstring>{
        
"\""    {
            yymore();
            BEGIN string;
        }

"<"     {
            yymore();
            BEGIN pstring;
        }

\n      {
            yyless(0);
            BEGIN INITIAL;
        }
            
.       {
            yyless(0);
            BEGIN xstring;
        }
}
